<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.emily.framework.datasource.mapper.MysqlMapper">
	<resultMap id="nodeMap" type="com.emily.framework.datasource.po.QuartzJob">
		<result column="JOB_NAME" property="jobName"/>
		<result column="JOB_GROUP" property="jobGroup"/>
	</resultMap>

	<select id="findQuartzJob" resultMap="nodeMap">
		SELECT qjd.JOB_NAME,qjd.JOB_GROUP FROM QUARTZ_JOB_DETAILS qjd WHERE qjd.SCHED_NAME='SmallGrainScheduler'
    </select>


</mapper>